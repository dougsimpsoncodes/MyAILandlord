{
  "name": "myailandlord",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "postinstall": "ios/scripts/patch-expo-headers.sh",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clear": "expo r -c",
    "doctor": "expo doctor",
    "typecheck": "npx tsc --noEmit",
    "security:audit": "./scripts/security-audit.sh",
    "security:check-secrets": "git log --all -S 'APIKey\\|secret\\|password' --oneline",
    "validate:env": "node -e \"console.log('✅ Environment validation:', process.env.EXPO_PUBLIC_SUPABASE_URL ? 'Supabase configured' : '❌ Missing Supabase config')\"",
    "deps:audit": "npm audit --audit-level=high",
    "deps:update": "npx expo install --fix",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:ts": "npx tsc --noEmit --strict",
    "prebuild:ios": "npm run typecheck && npm run security:audit",
    "prebuild:android": "npm run typecheck && npm run security:audit",
    "scan:secrets": "gitleaks detect --redact --config .gitleaks.toml --report-path .reports/gitleaks.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --coverage",
    "test:rls": "node scripts/ci/rls-smoke.js",
    "test:e2e": "./scripts/run-live-tests.sh quick",
    "test:e2e:auth": "./scripts/run-live-tests.sh auth",
    "test:e2e:critical": "./scripts/run-live-tests.sh critical",
    "test:e2e:full": "./scripts/run-live-tests.sh full",
    "test:e2e:mobile": "./scripts/run-live-tests.sh mobile",
    "test:e2e:maintenance": "./scripts/run-live-tests.sh maintenance",
    "scan:secrets:all": "gitleaks detect --no-git --source . --redact --config .gitleaks.toml --report-path .reports/gitleaks-all.json"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@supabase/supabase-js": "^2.56.0",
    "@upstash/redis": "^1.35.4",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~53.0.25",
    "expo-auth-session": "^6.2.1",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-image-loader": "~5.1.0",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-local-authentication": "~16.0.5",
    "expo-secure-store": "^14.2.4",
    "expo-speech": "^13.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.74",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.40.0"
  },
  "private": true
}
