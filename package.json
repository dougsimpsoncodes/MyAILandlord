{
  "name": "myailandlord",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clear": "expo r -c",
    "doctor": "expo doctor",
    "typecheck": "npx tsc --noEmit",
    "security:audit": "./scripts/security-audit.sh",
    "security:check-secrets": "git log --all -S 'APIKey\\|secret\\|password' --oneline",
    "validate:env": "node -e \"console.log('✅ Environment validation:', process.env.EXPO_PUBLIC_SUPABASE_URL ? 'Supabase configured' : '❌ Missing Supabase config')\"",
    "deps:audit": "npm audit --audit-level=high",
    "deps:update": "npx expo install --fix",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:ts": "npx tsc --noEmit --strict",
    "prebuild:ios": "npm run typecheck && npm run security:audit",
    "prebuild:android": "npm run typecheck && npm run security:audit",
    "scan:secrets": "gitleaks detect --redact --config .gitleaks.toml --report-path .reports/gitleaks.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --coverage",
    "test:rls": "node scripts/ci/rls-smoke.js",
    "test:e2e": "./scripts/run-live-tests.sh quick",
    "test:e2e:auth": "./scripts/run-live-tests.sh auth",
    "test:e2e:critical": "./scripts/run-live-tests.sh critical",
    "test:e2e:full": "./scripts/run-live-tests.sh full",
    "test:e2e:mobile": "./scripts/run-live-tests.sh mobile",
    "test:e2e:maintenance": "./scripts/run-live-tests.sh maintenance",
    "scan:secrets:all": "gitleaks detect --no-git --source . --redact --config .gitleaks.toml --report-path .reports/gitleaks-all.json",
    "lint:new-lines": "node scripts/ci/lint-new-lines.js",
    "test:unit:ci": "jest --passWithNoTests --testPathIgnorePatterns=src/__tests__/security/rls/",
    "stability:gate": "bash scripts/ci/stability-gate.sh",
    "test:manual:landlord": "./scripts/manual-test-first-time-landlord.sh",
    "test:manual:landlord:clean": "./scripts/manual-test-first-time-landlord.sh --clean",
    "qa:preflight": "./scripts/preflight-check.sh --mode device --connection tunnel",
    "qa:preflight:web": "./scripts/preflight-check.sh --mode web",
    "qa:preflight:device": "./scripts/preflight-check.sh --mode device --connection tunnel",
    "qa:golden": "./scripts/qa-golden.sh --mode device --connection tunnel",
    "qa:golden:web": "./scripts/qa-golden.sh --mode web",
    "qa:golden:device": "./scripts/qa-golden.sh --mode device --connection tunnel",
    "dev:web:start": "./scripts/e2e-web-server.sh start",
    "dev:web:stop": "./scripts/e2e-web-server.sh stop",
    "dev:web:status": "./scripts/e2e-web-server.sh status",
    "dev:web:logs": "./scripts/e2e-web-server.sh logs",
    "test:rw": "./scripts/run-playwright-real-world.sh",
    "test:rw:headed": "PW_HEADLESS=0 ./scripts/run-playwright-real-world.sh --headed",
    "test:rw:debug": "PWDEBUG=1 PW_HEADLESS=0 ./scripts/run-playwright-real-world.sh --debug",
    "test:rw:report": "npx playwright show-report artifacts/playwright/report",
    "dev:patch:safe": "./scripts/safe-apply-patch.sh",
    "test:rw:manual": "PW_SPEC=tests/e2e/manual-parity-landlord-tenant.spec.ts ./scripts/run-playwright-real-world.sh",
    "test:rw:manual:headed": "PW_HEADLESS=0 PW_SPEC=tests/e2e/manual-parity-landlord-tenant.spec.ts ./scripts/run-playwright-real-world.sh --headed",
    "test:rw:fast": "PW_SPEC=tests/e2e/invite-landlord-tenant.spec.ts ./scripts/run-playwright-real-world.sh",
    "test:pw:chromium": "PW_HEADLESS=1 ./scripts/run-playwright-suite.sh --browser=chromium --reporter=list",
    "test:pw:firefox": "PW_HEADLESS=1 ./scripts/run-playwright-suite.sh --browser=firefox --reporter=list",
    "test:pw:all": "npm run test:pw:chromium && npm run test:pw:firefox"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@supabase/supabase-js": "^2.56.0",
    "@upstash/redis": "^1.35.4",
    "base64-arraybuffer": "^1.0.2",
    "expo": "~54.0.0",
    "expo-auth-session": "~7.0.10",
    "expo-barcode-scanner": "^13.0.1",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-local-authentication": "~17.0.8",
    "expo-notifications": "~0.32.15",
    "expo-secure-store": "~15.0.8",
    "expo-speech": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-get-random-values": "^2.0.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.34.0",
    "@expo/ngrok": "^4.1.3",
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^25.0.3",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.16",
    "react-test-renderer": "^19.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "private": true
}
